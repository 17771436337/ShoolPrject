<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="prject.shool.src.com.shoolprject">


    <!-- 网络权限 -->
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".App"
        android:allowBackup="true"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">

        <!-- 首页 -->
        <activity
            android:name=".activity.app.MainActivity"
            android:label="首页"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity
            android:name=".activity.app.SettingActivity"
            android:label="设置"></activity>

        <activity
            android:name=".activity.WebViewActivity"
            android:label="网络视图"></activity>
        <activity
            android:name=".activity.TextActivity"
            android:label="数据测试"></activity>
    </application>

</manifest>